{
    "name": "bazaarAT",
    "description": "Simple Nginx site configuration",
    "version": "0.0.1",
    "author": "https://github.com/absidish",
    "containers": [
        {
            "hostname": "nginxbazaarat",
            "template": "debian-buster",
            "peer-criteria": "HTTP-GROUP",
            "size": "TINY",
            "port-mapping": [
                {
                    "protocol": "https",
                    "domain": "iuk.envs.subut.ai",
                    "internal-port": "80",
                    "external-port": "443"
                }
            ]
        }
    ],
    "peer-criteria": [
        {
            "name": "HTTP-GROUP"
        }
    ],
    "ansible-configuration": {
        "source-url": "https://github.com/absidish/simple-nginx-app/archive/main.zip",
        "ansible-playbook": "site.yml",
        "extra-vars": [
        ],
        "groups": [
            {
                "name": "nginx-container",
                "hostnames": [
                    "nginxbazaarat"
                ]
            }
        ]
    },
    "user-variables": {
    }
}
